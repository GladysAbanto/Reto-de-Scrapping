{
  "manifest_version": 3,
  "name": "Reto Scrapper",
  "version": "1.0.0",
  "action": {
    "default_title": "Click para scrapear",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "sw.js",
    "type": "module"
  },
  "permissions": ["activeTab", "tabs", "scripting", "storage"]
}
